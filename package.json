{
  "name": "htha",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate --config drizzle.config.ts",
    "db:reset": "rm -rf ./pglite && npm run db:up",
    "db:seed": "node --experimental-strip-types ./src/database/seed.ts",
    "db:up": "npm run db:migrate && npm run db:seed",
    "start": "node --experimental-strip-types --watch-path=./src ./src/app.ts",
    "test": "vitest",
    "test:cov": "vitest run --coverage --watch"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@electric-sql/pglite": "0.3.10",
    "drizzle-orm": "0.44.5",
    "fastify": "5.6.1"
  },
  "devDependencies": {
    "@types/node": "24.6.0",
    "@vitest/coverage-istanbul": "3.2.4",
    "drizzle-kit": "0.31.5",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "imports": {
    "#modules/constants/*.ts": "./src/constants/*.ts",
    "#modules/database/*.ts": "./src/database/*.ts",
    "#modules/properties/*.ts": "./src/properties/*.ts",
    "#types/*.ts": "./types/*.ts"
  }
}
